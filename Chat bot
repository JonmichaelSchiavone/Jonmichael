    "import datetime\n",
    "datetime.datetime.now()\n",
    "datetime.datetime(2009, 6, 15, 8, 24,).strftime(\"%I:%M%p on %B %d, %Y\")\n",
    "\n",
    "\n",
    "#start chat\n",
    "print(datetime.datetime.now().strftime(\"%I:%M%p\") +'\\nPanther Bot: Hi I am Panther bot, I will answer your questions about Adelphi.')\n",
    "speak.Speak('Hi I am Panther bot, I will answer your questions about Adelphi')\n",
    "\n",
    "exit_list = ['exit', 'see you later', 'bye', 'quit', 'break', 'goodbye', 'good bye']\n",
    "\n",
    "while(True):\n",
    "    user_input = input('\\n'+datetime.datetime.now().strftime(\"%I:%M%p\") +'\\nYou: ')\n",
    "    if user_input.lower() in exit_list:\n",
    "        print('\\n'+datetime.datetime.now().strftime(\"%I:%M%p\") +'\\nPanther Bot: Chat with you later!')\n",
    "        speak.Speak('Chat with you later')\n",
    "        break\n",
    "    else:\n",
    "        if greeting_response(user_input) != None:\n",
    "            print('\\n'+datetime.datetime.now().strftime(\"%I:%M%p\") +'\\nPanther Bot: ' +greeting_response(user_input)) \n",
    "            speak.Speak(greeting_response(user_input))\n",
    "        else: \n",
    "            print('\\n' +datetime.datetime.now().strftime(\"%I:%M%p\") + '\\nPanther Bot: ' +bot_response(user_input))\n",
    "            speak.Speak(bot_response(user_input))\n",
