   "source": [
    "#Function to return a random greeting response to USER GREETING\n",
    "def greeting_response(text):\n",
    "    text = text.lower()\n",
    "    \n",
    "    #Bots Greeting response\n",
    "    bot_greetings = ['Hello']\n",
    "    \n",
    "    #user Greeting\n",
    "    user_greetings = ['hi', 'hey', 'hello', 'hola', 'greetings', 'wassup', 'howdy']\n",
    "    \n",
    "    #Returning a randomly chosen greeting\n",
    "    for word in text.split():\n",
    "        if word in user_greetings:\n",
    "            return random.choice(bot_greetings) \n",
    "            return speak.Speak(random.choice(bot_greetings))\n",
    "            "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 103,
   "id": "35845644",
   "metadata": {},
   "outputs": [],
   "source": [
    "def thank_response(text):\n",
    "    text = text.lower()\n",
    "    \n",
    "    #Bots Greeting response\n",
    "    bot_thank = ['you are very welcome, is there anything else I can help you with']\n",
    "    \n",
    "    #user Greeting\n",
    "    user_thank = ['thank you', 'thanks', 'i appreciate it', 'thank you so much', 'ty', 'gracias']\n",
    "    \n",
    "    #Returning a randomly chosen greeting\n",
    "    for word in text.split():\n",
    "        if word in user_greetings:\n",
    "            return random.choice(bot_thank) \n",
    "            return speak.Speak(random.choice(bot_thank))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 104,
   "id": "8944cbd7",
   "metadata": {},
   "outputs": [],
   "source": [
    "def index_sort(list_var):\n",
    "    length = len(list_var)\n",
    "    list_index = list(range(0, length))\n",
    "    \n",
    "    x = list_var\n",
    "    \n",
    "    #sorting the list\n",
    "    for i in range(length):\n",
    "        for j in range(length):\n",
    "            if x[list_index[i]] > x[list_index[j]]:\n",
    "                #Swap\n",
    "                temp = list_index[i]\n",
    "                list_index[i] = list_index[j]\n",
    "                list_index[j] = temp \n",
    "                \n",
    "    return list_index"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 165,
   "id": "ec06ae57",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "2022-05-04 16:59:27.265562\n"
     ]
    }
   ],
   "source": [
    "\n",
    "#creating bot response\n",
    "def bot_response(user_input):\n",
    "    user_input = user_input.lower()\n",
    "    sentence_list_Final1.append(user_input)\n",
    "    \n",
    "    bot_response = ''\n",
    "    \n",
    "    #Getting sentence from count matrix\n",
    "    cm = CountVectorizer().fit_transform(sentence_list_Final1)\n",
    "    #Taking the last users input and comparing it in everything in count matrix\n",
    "    similarity_scores = cosine_similarity(cm[-1],cm)\n",
    "    #reduce dimensionality of the similarity scores\n",
    "    similarity_scores_list = similarity_scores.flatten()\n",
    "    \n",
    "    #finding the index in the highest score\n",
    "    index = index_sort(similarity_scores_list)\n",
    "    \n",
    "    #contain only values that are not itself\n",
    "    index = index[1:]\n",
    "    #see if there is a response similar to the user \n",
    "    response_flag = 0\n",
    "    \n",
    "    j = 0 \n",
    "    \n",
    "    for i in range(len(index)):\n",
    "        if similarity_scores_list[index[i]]> 0.0:\n",
    "            bot_response = bot_response + ' ' + sentence_list_Final1[index[i]]\n",
    "            response_flag = 1\n",
    "            #how many scores there are above 0\n",
    "            j = j + 1 \n",
    "        if j > 2:\n",
    "            break\n",
    "    #response flag 0 did not find a sentence similar to input\n",
    "    if response_flag == 0:\n",
    "        bot_response = bot_response + '' + \"I apologize, I do not understand.\\n\"  \n",
    "        speak.Speak('I apologize, I do not understand')\n",
    "        \n",
    "    #remove user reponse from the sentence list\n",
    "    sentence_list_Final1.remove(user_input)\n",
    "    return bot_response"
   ]
  },
